{
  "compilerOptions": {
    "target": "ES2016",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "paths": {
      "Clients": ["./src/clients"],
      "Models/*": ["./src/models/*"],
      "Controllers/*": ["./src/controllers/*"],
      "Utils/*": ["./src/utils/*"],
      "Constants/*": ["./src/constants/*"],
      "Middleware/*": ["./src/middleware/*"],
      "Routes/*": ["./src/routes/*"],
      "GraphqlResolver": ["./src/graphql/resolvers"],
      "GraphqlTypeDefs": ["./src/graphql/typeDefs"]
    }
  }
}
